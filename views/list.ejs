<%-include("header")-%>
  <div class="box" id="heading">
    <h1> <%= Title %></h1>
  </div>

<div class="box">

    <% for(var i=0;i<Item.length;i++) { %>
      <div class="item">
        <input type="checkbox">
        <p> <%= Item[i] %> </p>

      </div>
<% }  %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New item">
        <button type="submit" name="button" value=<%= Title %>> + </button>
      </form>

</div>
<%-include("footer")-%>
